{
  "name": "react-modal-wrapper",
  "version": "0.1.14",
  "description": "A React Modal Wrapper that uses FlexBox to keep it's position, BYOM (bring-your-own-modal)",
  "main": "dist/index.js",
  "scripts": {
    "test": "webpack --config ./webpack.test.js --target node && mocha tmp/test.js",
    "start": "webpack-dev-server --inline --config ./webpack.config.js --content-base examples/ \"$@\"",
    "prepublish": "npm test && webpack --devtool source-map --config ./webpack.build.js"
  },
  "author": "1stdibs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/1stdibs/react-modal-wrapper"
  },
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-portal": "~1.5.5"
  },
  "peerDependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^6.1.18",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-2": "^6.1.18",
    "css-loader": "^0.23.0",
    "es6-promise": "^3.0.2",
    "null-loader": "^0.1.1",
    "react-addons-css-transition-group": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.8",
    "webpack-dev-server": "^1.12.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ]
  },
  "keywords": [
    "react",
    "flex",
    "modal",
    "flexbox",
    "wrapper"
  ]
